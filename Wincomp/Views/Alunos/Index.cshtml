@model IEnumerable<Wincomp.Models.Aluno>
@{
    ViewBag.Title = "Index";
}

<h2>I can hear you</h2>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model =>model.NomeAluno )</th>
            <th>@Html.DisplayNameFor(model => model.Email)</th>
            <th>@Html.DisplayNameFor(model => model.Data)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.NomeAluno)</td>
            <td>@Html.DisplayFor(modelItem => item.Email)</td>
            <td>@Html.DisplayFor(modelItem => item.Data)</td>
        </tr>
        }
    </tbody>
</table>
